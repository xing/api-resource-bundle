{
    "name": "xing/api-resource-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides easy to use API resources for serialization and deserialization.",
    "homepage": "https://xing.com",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "Xing\\ApiResourceBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xing\\ApiResourceBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "symfony/config": "^4.4|^5.0|^6.4",
        "symfony/http-kernel": "^4.4|^5.0|^6.4",
        "doctrine/orm": "^2.7",
        "symfony/dependency-injection": "^4.4|^5.0|^6.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "authors": [
        {
            "name": "Florian Hofinger",
            "email": "florian.hofinger@xing.com"
        }
    ]
}
