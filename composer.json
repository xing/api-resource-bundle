{
    "name": "ps/api-resource-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides easy to use API resources for serialization and deserialization.",
    "homepage": "https://prescreen.io",
    "license": "MIT",
    "config": {
        "platform": {
            "php": "8.4"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "Prescreen\\ApiResourceBundle\\": ""
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/config": "^4.4",
        "symfony/http-kernel": "^4.4",
        "doctrine/orm": "^2.7",
        "symfony/dependency-injection": "^4.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "authors": [
        {
            "name": "Florian Hofinger",
            "email": "florian.hofinger@prescreen.io"
        }
    ]
}
